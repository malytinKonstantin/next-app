    "@emotion/styled": "^11.10.0",
    "classnames": "^2.3.1",
    "lodash.debounce": "4.0.8",
    "mobx": "^6.6.2",
    "mobx-react": "^7.5.3",
    "nanoid": "^4.0.0",
    "next": "12.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.29.0",
    "react-transition-group": "^4.4.5",
    "react-yandex-maps": "4.6.0",
    "slick-carousel": "^1.8.1",
    "storybook-addon-next": "^1.6.7"
  },
