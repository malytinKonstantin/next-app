{
  "name": "top-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "stylelint": "stylelint \"**/*.{ts,tsx,css}\" --fix",
    "format": "prettier --write .",
    "storybook": "start-storybook -p 6006 ./public",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "stylelint \"**/*.{ts,tsx,css}\"  --fix",
      "prettier --write .",
      "next lint --fix",
      "git add"
    ],
    "*.{html,css}": [
      "stylelint \"**/*.{ts,tsx,css}\" --fix",
      "prettier --write .",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
